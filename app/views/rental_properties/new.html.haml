#main.container
    .form
        =form_for RentalProperty.new do |f|
            =f.label :title
            =f.text_field :title

            =f.label :description
            =f.text_field :description

            =f.label :address
            =f.text_field :address

            =f.label "E-mail"
            =f.text_field :email

            =f.label "Price"
            =f.number_field :price

            =f.label "Image"
            =f.file_field :images

            =f.submit



    .row
        .middle
            .button-navigate= link_to('Back to rental properties list', rental_properties_path)
